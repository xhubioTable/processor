{
  "name": "@xhubiotable/processor",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "description": "",
  "keywords": [
    "decision-table",
    "equivalence-class-table",
    "data generator",
    "test data",
    "test"
